{
  "layer": "top",
  "position": "top",
  "margin": 0,
  "spacing": 5,
  "modules-left": ["sway/workspaces", "sway/mode"],
  "modules-center": ["clock"],
  "modules-right": ["pulseaudio", "memory", "cpu", "battery", "network", "tray"],
  "sway/workspaces": {
    "all-outputs": true,
    "disable-scroll": false
  },
  "clock": {
    "format": "%a %b %d | %H:%M",
    "tooltip-format": "%A, %d %B %Y"
  },
  "pulseaudio": {
